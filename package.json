  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "standard-changelog": "^2.0.21",
    "tap-nirvana": "^1.1.0",
    "tape": "^4.13.2"
  },
  "scripts": {
    "build": "env BABEL_ENV=development rollup -c",
    "lint": "eslint src",
