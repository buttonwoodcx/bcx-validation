{
  "name": "bcx-validation",
  "version": "2.0.3",
  "description": "Buttonwood validator",
  "keywords": [
    "validation"
  ],
  "homepage": "http://github.com/buttonwoodcx/bcx-validation",
  "bugs": {
    "url": "https://github.com/buttonwoodcx/bcx-validation/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/buttonwoodcx/bcx-validation"
  },
  "license": "MIT",
  "author": "Chunpeng Huo",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "@babel/cli": "^7.14.5",
    "@vercel/ncc": "^0.28.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.30.0",
    "standard-changelog": "^2.0.27",
    "tap-nirvana": "^1.1.0",
    "tape": "^5.2.2"
  },
  "scripts": {
    "prebuild": "ncc build src/index.js -m -e contextual-proxy -e scoped-eval -e lodash -o packed",
    "build": "babel packed/index.js -o dist/index.js",
    "lint": "eslint src",
    "prepare": "npm run build",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish --tag next",
    "pretest": "npm run lint",
    "test": "tape -r @babel/register 'test/**/*.spec.js' | tap-nirvana"
  },
  "main": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "dependencies": {
    "contextual-proxy": "^0.2.0",
    "lodash": "^4.17.21",
    "scoped-eval": "^0.2.0"
  }
}
