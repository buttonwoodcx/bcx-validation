    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "test": "env BABEL_ENV=test tape -r @babel/register 'test/**/*.spec.js' | tap-nirvana"
  },
  "main": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
